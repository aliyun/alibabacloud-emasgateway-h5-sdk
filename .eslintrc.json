{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "commonjs": true
  },
  "rules": {
    "no-empty": 0,
    "comma-dangle": 0,
    "no-unused-vars": 0,
    "no-console": 1,
    "no-cond-assign": 0,
    "no-unreachable": 0,
    "no-extra-boolean-cast": 0,
    "no-extra-semi": 0
  }
}